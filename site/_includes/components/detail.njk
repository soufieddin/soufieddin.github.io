<div class="cases container">
  <img class = "image--client" src = "../../static/images/{{project.thumb}}.svg" alt='logo of project'/>
  <div class="cases__text">
    <h2 class = "cases__text__title">{{project.title}}</h2>
  </div>
  <div class="cases__client">
    <div class = "row">
      <div class = "cases__client__about col-md-6 col-sm-12">
        <h6>Developed & designed as a university project </h6>
        <p>{{project.synopsis}}</p>
      </div>
      <div class = "cases__client__tech col-md-6 col-sm-12">
        <h6>Used technologies</h6>
        <div>
          {% for t in technologies %}
          <strong>{{t}}</strong> <br/>
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
  
</div>
<div class="cases__element container">
  <div class="cases__element__subtitle">
    <a class = "cases__element__subtitle__content" href="{{project.link}}" target="_blank">Visit Site</a>
  </div>
  <div class="row cases__element__item">
    <div class="cases__element__item__img gf__img col-sm-12 col-lg-3">
      <img src="../../static/images/{{project.mob}}.png" alt='headpage of project on mobile mockup'/>
    </div>
    <div class="cases__element__item__img gf__img col-sm-12 col-lg-4">
      <img src="../../static/images/{{project.ipad}}.png" alt='headpage of project on ipad mockup'/>
    </div>
    <div class="cases__element__item__img gf__img col-sm-12  col-lg-5">
      <img src="../../static/images/{{project.pc}}.png" alt='headpage of project on pc mockup'/>
    </div>
  </div>
</div>